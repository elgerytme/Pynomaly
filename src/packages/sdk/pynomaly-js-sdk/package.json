{
  "name": "@pynomaly/js-sdk",
  "version": "1.0.0",
  "type": "module",
  "description": "Official JavaScript/TypeScript SDK for Pynomaly data science platform",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "build:node": "rollup -c --environment TARGET:node",
    "build:browser": "rollup -c --environment TARGET:browser",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:node": "jest --testEnvironment=node",
    "test:browser": "jest --testEnvironment=jsdom",
    "test:compatibility": "node examples/cross-platform-usage.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "dev": "rollup -c -w",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run type-check && npm run test && npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "pynomaly",
    "data-science",
    "anomaly-detection",
    "machine-learning",
    "typescript",
    "javascript",
    "sdk",
    "web"
  ],
  "author": "Pynomaly Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elgerytme/Pynomaly.git",
    "directory": "src/packages/sdk/pynomaly-js-sdk"
  },
  "bugs": {
    "url": "https://github.com/elgerytme/Pynomaly/issues"
  },
  "homepage": "https://pynomaly.com",
  "dependencies": {
    "axios": "^1.7.9",
    "eventemitter3": "^5.0.1"
  },
  "optionalDependencies": {
    "node-fetch": "^3.3.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.8",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "rollup": "^4.45.1",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@angular/core": ">=15.0.0",
    "react": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
