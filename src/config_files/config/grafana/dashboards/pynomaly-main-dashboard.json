{
  "dashboard": {
    "id": null,
    "title": "Pynomaly Production Dashboard",
    "tags": ["pynomaly", "production", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"pynomaly-api\"}",
            "legendFormat": "API Status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "API Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pynomaly_requests_total[5m])",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Anomaly Detection Rate",
        "type": "singlestat",
        "targets": [
          {
            "expr": "rate(pynomaly_detections_total[5m])",
            "legendFormat": "Detections/sec",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "singlestat",
        "targets": [
          {
            "expr": "rate(pynomaly_errors_total[5m])",
            "legendFormat": "Errors/sec",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "thresholds": "0.1,0.5",
        "colorBackground": true,
        "colors": ["green", "yellow", "red"],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Response Time",
        "type": "singlestat",
        "targets": [
          {
            "expr": "pynomaly_processing_time_ms",
            "legendFormat": "Avg Response Time (ms)",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "ms",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_activity_count",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Connections",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"pynomaly-api\"}/1024/1024",
            "legendFormat": "Memory Usage (MB)",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "MB",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"pynomaly-api\"}[5m])*100",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Percent",
            "show": true,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Redis Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_commands_total[5m])",
            "legendFormat": "Commands/sec",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Commands/sec",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "System Uptime",
        "type": "singlestat",
        "targets": [
          {
            "expr": "pynomaly_uptime_seconds",
            "legendFormat": "Uptime",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "s",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 11,
        "title": "Disk Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (node_filesystem_avail_bytes{mountpoint=\"/\"}/node_filesystem_size_bytes{mountpoint=\"/\"})*100",
            "legendFormat": "Disk Usage %",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Percent",
            "show": true,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 32
        }
      },
      {
        "id": 12,
        "title": "Network I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(node_network_receive_bytes_total[5m])*8",
            "legendFormat": "Receive",
            "refId": "A"
          },
          {
            "expr": "rate(node_network_transmit_bytes_total[5m])*8",
            "legendFormat": "Transmit",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "bits/sec",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 32
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "refresh": "30s",
    "schemaVersion": 16,
    "version": 1
  }
}
